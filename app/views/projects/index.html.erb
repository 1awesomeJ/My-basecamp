
<%= link_to "New project", new_project_path, class: "btn btn-dark"%>
</br></br>
<h1>Projects</h1>
<b>These are all the current projects:</b>
</br></br>
<div id="projects">
  <table class = "table table-stripped table-bordered table-hover">
    <thead class = "thead-dark">
      <% @projects.each do |project| %>
    <%= render project %>

 
    <p>
      <%= link_to "Show this project", project, class: "btn btn-primary" %>
    </p>
    </br>
  <% end %>
    </thead>
    </table>
</div>
